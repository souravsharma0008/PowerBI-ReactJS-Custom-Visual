{
  "compilerOptions": {
      "jsx": "react-jsx",
      "types": ["react", "react-dom"],
      "allowJs": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es2022",
      "sourceMap": true,
      "outDir": "./.tmp/build/",
      "moduleResolution": "node",
      "declaration": true,
      "lib": ["es2015", "dom"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "files": ["src/visual.ts"]
}
